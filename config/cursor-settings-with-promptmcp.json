{
  "mcp.servers": {
    "promptmcp-local": {
      "command": "node",
      "args": [
        "C:\\cursor\\vibe\\dist\\mcp\\server.js"
      ],
      "env": {
        "NODE_ENV": "production",
        "CONTEXT7_ENABLED": "true",
        "LOG_LEVEL": "info"
      },
      "stdio": true,
      "description": "PromptMCP Local - Simple prompt enhancement tool"
    },
    "promptmcp-docker": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "promptmcp:latest"
      ],
      "env": {
        "NODE_ENV": "production",
        "CONTEXT7_ENABLED": "true",
        "LOG_LEVEL": "info"
      },
      "stdio": true,
      "description": "PromptMCP Docker - Containerized simple prompt enhancement"
    }
  },
  "mcp.enabled": true,
  "mcp.defaultServer": "promptmcp-local"
}